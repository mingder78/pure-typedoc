{
  "name": "@yourorg/pure-typedoc",
  "version": "0.0.1",
  "description": "A template for typedoc",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch ./app.ts",
    "start": "bun ./app.ts",
    "build": "bun run build.mjs",
    "type-docs": "typedoc",
    "show-docs": "npx serve docs/api",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "bun run build && bun type-docs"
  },
  "exports": {
    ".": "./dist/index.js",
    "./Todo": "./dist/modules/Todo.js",
    "./User": "./dist/modules/User.js"
  },
  "files": [
    "dist",
    "docs/api",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "vitest",
    "typedoc",
    "react",
    "bun"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "bun-plugin-dts": "^0.3.0",
    "typedoc": "^0.28.14"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "typedocOptions": {
    "entryPointStrategy": "expand",
    "entryPoints": ["src"],
    "out": "docs/api",
    "theme": "default",
    "includeVersion": true,
    "hideGenerator": true
  },
  "dependencies": {
    "@libp2p/identify": "^4.0.9",
    "@libp2p/logger": "^6.2.2",
    "@libp2p/ping": "^3.0.9",
    "libp2p": "^3.1.2"
  }
}
