{
  "name": "@yourorg/pure-typedoc",
  "version": "0.0.1",
  "description": "A template for typedoc",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rimraf ./docs/api dist",
    "dev": "bun --watch ./app.ts",
    "start": "bun ./app.ts",
    "build": "bun run build.mjs",
    "type-docs": "typedoc",
    "show-docs": "npx serve docs/api",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "bun clean && bun run build && bun type-docs"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "docs/api",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "vitest",
    "typedoc",
    "react",
    "bun"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "@vitest/coverage-v8": "4.0.13",
    "@vitest/ui": "4.0.13",
    "bun-plugin-dts": "^0.3.0",
    "rimraf": "^6.1.2",
    "typedoc": "^0.28.14"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "typedocOptions": {
    "entryPointStrategy": "expand",
    "entryPoints": ["src"],
    "out": "docs/api",
    "theme": "default",
    "includeVersion": true,
    "hideGenerator": true
  },
  "dependencies": {
    "@libp2p/identify": "^4.0.9",
    "@libp2p/logger": "^6.2.2",
    "@libp2p/ping": "^3.0.9",
    "libp2p": "^3.1.2",
    "vitest": "^4.0.13"
  }
}
